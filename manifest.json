{
	"manifest_version": 2,
	"name": "Untab",
	"description": "A simple chrome based extension to save the URLs of currently opened tabs",
	"version": "1.0",
	"icons": {
		"48": "img/icon48.png"
	},
	"browser_action": {
		"default_icon": "img/icon48.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"storage"
	]
	
}